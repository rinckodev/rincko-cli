                        { "label": "MySql", "value": "mysql", "hint": "Bot with MySql database"  },
                        { "label": "QuickDb", "value": "quickdb", "hint": "Bot with QuickDb local database" }
                    ]
                },
                "includeExamples":{
                    "message": "Add usage examples?",
                    "defaultValue": true
                },
                "installDep":{
                    "message": "Install dependencies?",
                    "defaultValue": true
                },
                "installDepSpinner":{
                    "start": "Installing dependencies",
                    "stop": "Dependencies installed"
                },
                "note":{
                    "title": "Done",
                    "notes": {
                        "cd":"cd ${path}",
                        "dependencies":"npm install",
                        "dev":"npm run dev",
                        "readme":"See project README.md"
                    }
                },
                "outro": "Questions? http://discord.gg/tTu8dGN"
            }
        }
    },
